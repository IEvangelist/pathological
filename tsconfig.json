{
	"include": [
		"src"
	],
	"compilerOptions": {
		"module": "CommonJS",
		"target": "ES2020",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"sourceMap": true,
		"strict": true,
		"moduleResolution": "node",
		"outDir": "out",
		"resolveJsonModule": true,
		"noImplicitReturns": true,
		"noUnusedParameters": true
	},
	"exclude": [
		"node_modules",
		".vscode-test"
	]
}